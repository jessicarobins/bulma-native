{
  "name": "@jrobins/bulma-native",
  "description": "React Native port of Bulma components",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist",
    "README.md"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "sb:build": "rm -rf ./sb-dist && build-storybook -o ./sb-dist -c ./.storybook-web",
    "sb:start": "start-storybook -p 9001 -c ./.storybook-web",
    "sb:deploy": "STORYBOOK_GA_ID='UA-166022777-1' yarn sb:build && gh-pages -d ./sb-dist",
    "lint": "eslint . --ext .ts,.tsx",
    "prepublish": "rm -rf dist/ && yarn build",
    "prettier": "prettier --check \"{shared,src}/**/*.{ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,tsx}\"",
    "ts:lint": "tsc --noEmit",
    "ts:watch": "tsc -w"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "dependencies": {
    "chroma-js": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/loader": "^1.6.1",
    "@react-native-community/eslint-config": "^1.1.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-google-analytics": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-ondevice-actions": "^5.3.18",
    "@storybook/addon-ondevice-knobs": "^5.3.18",
    "@storybook/addon-options": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/cli": "^5.3.18",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.18",
    "@storybook/react-native": "^5.3.18",
    "@storybook/source-loader": "^5.3.18",
    "@storybook/theming": "^5.3.18",
    "@types/chroma-js": "^2.0.0",
    "@types/react": "~16.9.23",
    "@types/react-native": "~0.61.17",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-native-web": "^0.12.2",
    "babel-preset-expo": "~8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "expo": "~37.0.3",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "gh-pages": "^2.2.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.4",
    "react": "~16.9.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-screens": "~2.2.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "^0.12.2",
    "react-syntax-highlighter": "^12.2.1",
    "rollup": "^2.8.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "storybook-addon-jsx": "^7.2.3",
    "typescript": "~3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0"
  },
  "private": false,
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globalMock.js"
    ]
  },
  "version": "0.0.4"
}
